<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/register_bg"
    android:orientation="vertical"

    >
    <com.tentinet.healthy.view.TitleView
        android:id="@+id/health_assess_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.tentinet.healthy.widget.PageLoadIng
        android:id="@+id/data_page_load"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.tentinet.healthy.widget.PageLoadIng>

    <TextView
        android:id="@+id/txt_health_explain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/health_assess_head"
        android:padding="@dimen/padding_default"
        android:text="@string/healty_assess_head_msg"
        android:textSize="@dimen/dp_15" />

    <lecho.lib.hellocharts.view.LineChartView
        android:id="@+id/char_health_assess"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:paddingBottom="@dimen/padding_default"
        android:paddingTop="@dimen/padding_default" />

    <GridView
        android:id="@+id/gid_health_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:numColumns="2"
        android:smoothScrollbar="true"
        android:soundEffectsEnabled="true"
        android:stretchMode="columnWidth"></GridView>

    <TextView
        android:id="@+id/txt_health_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_3"
        android:background="@color/white"
        android:padding="@dimen/padding_default"
        android:text="@string/healty_assess_foot_msg"
        android:textSize="@dimen/dp_15" />

</LinearLayout>
