<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/health_assess_boby"
        android:gravity="center_vertical"
        android:orientation="vertical">

        <TextView
            android:id="@+id/health_txt_name"
            style="@style/data_item_txt_style_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/padding_default"
            android:paddingLeft="@dimen/padding_default_for_horizontal"
            android:paddingTop="@dimen/padding_default"

            android:textColor="@color/data_title" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/padding_default"
            android:paddingLeft="@dimen/padding_default_for_horizontal"
            android:paddingTop="@dimen/padding_default">

            <TextView
                android:id="@+id/health_txt_result"
                style="@style/data_item_txt_style_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textSize="@dimen/dp_20"

                />

            <TextView
                android:id="@+id/health_txt_result_msg"
                style="@style/data_item_txt_style_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:textSize="@dimen/dp_15"

                />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

