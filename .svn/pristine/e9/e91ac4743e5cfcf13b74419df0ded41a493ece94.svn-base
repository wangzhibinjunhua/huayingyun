<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/txt_date"
        style="@style/data_item_txt_style_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/padding_default" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/register_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            style="@style/data_item_txt_style_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="@dimen/padding_default"
            android:text="@string/blood_pressure_measurement" />


    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/register_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/dp_20"
        android:paddingLeft="@dimen/padding_default"
        android:paddingTop="@dimen/dp_20">

        <TextView
            style="@style/data_item_txt_style_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:text="@string/high_pressure" />

        <TextView
            android:id="@+id/txt_high_pressure"
            style="@style/txt_style_default"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:textColor="@color/font_color_green_default"
            android:textSize="@dimen/dp_30" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/dp_20"
        android:paddingLeft="@dimen/padding_default"
        android:paddingTop="@dimen/dp_20">

        <TextView
            style="@style/data_item_txt_style_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:text="@string/low_pressure" />

        <TextView
            android:id="@+id/txt_low_pressure"
            style="@style/txt_style_default"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:textColor="@color/font_color_green_default"
            android:textSize="@dimen/dp_30" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/dp_20"
        android:paddingLeft="@dimen/padding_default"
        android:paddingTop="@dimen/dp_20">

        <TextView
            style="@style/data_item_txt_style_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:text="@string/heart_rate" />

        <TextView
            android:id="@+id/txt_heart_rate"
            style="@style/txt_style_default"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:textColor="@color/data_pulse"
            android:textSize="@dimen/dp_30" />
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/register_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/padding_default"
        android:paddingLeft="@dimen/padding_default"
        android:paddingTop="@dimen/padding_default" >

        <TextView
            style="@style/data_item_txt_style_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/measurement_result_msg" />

        <TextView
            android:id="@+id/txt_measure_result"
            style="@style/data_item_txt_style_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
           />
    </LinearLayout>
</LinearLayout>
