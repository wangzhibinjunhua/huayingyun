<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

   <com.tentinet.healthy.view.TitleView
       android:id="@+id/measure_info_title"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"></com.tentinet.healthy.view.TitleView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        >


        <com.tentinet.healthy.ecg.BackGround
            android:id="@+id/main_pc80B_view_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <TextView
            android:id="@+id/main_pc80B_MSG"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/font_color_default"
            android:textSize="24sp" />

        <com.tentinet.healthy.ecg.DrawThreadPC80B
            android:id="@+id/main_pc80B_view_draw"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
</LinearLayout>
